{
  "name": "eslint-plugin-tags-validator",
  "version": "1.0.4",
  "description": "Validates tags applied to Mocha tests, primarily as defined using @cypress/grep",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/scottmcginness/eslint-plugin-tags-validator.git"
  },
  "author": "Scott McGinness",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslintplugin",
    "mocha",
    "cypress",
    "grep",
    "tag",
    "filter"
  ],
  "main": "index.js",
  "files": [
    "LICENSE.txt",
    "README.md",
    "index.js",
    "types.d.ts",
    "lib/**",
    "rules/**"
  ],
  "scripts": {
    "format:pkg": "format-package -w",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepack": "npm run lint",
    "test": "mocha ./test/**/*.test.js",
    "test:watch": "npm run test -- --watch"
  },
  "types": "types.d.ts",
  "dependencies": {
    "autocorrect": "^1.2.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/eslint": "^8.21.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.3",
    "@types/sinon": "^10.0.14",
    "@types/sinon-chai": "^3.2.9",
    "chai": "^4.3.7",
    "eslint": "^8.36.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-eslint-plugin": "^5.0.8",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-node": "^11.1.0",
    "format-package": "^7.0.0",
    "mocha": "^10.2.0",
    "sinon": "^15.0.4",
    "sinon-chai": "^3.7.0",
    "unit-test-sentinel": "^1.0.1"
  }
}
